cat < file < test | cat -e																						<- в симпл команду попал аргумент <LESS> и test
echo 0 > out0 > echo 1 > out1 > echo 2 > out2 > echo 3 > out3 > echo 4 > out4 > echo 5 > out5 					<- выскочил санитайзер (когда 3 и более любых редиректа)
echo hello world >> 1 >> 2 >> 3																					<- выскочил санитайзер
echo hello world > 1 > 2 > 3																					<- выскочил санитайзер
echo hello world < 1 < 2 < 3																					<- выскочил санитайзер
echo test < t1 < t2																								<- в лексер попал какой-то мусор (в начало аргумента test)
																												<- крашится при пустой команде
