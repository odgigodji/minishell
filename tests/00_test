[passed]
'e'cho  1 2 3 +
echo '\\\\' ; echo "\\\\" +
echo "$PWD" +
echo '$PWD' +
echo "\\n"      +
 export t=u; echo $t  отбраотало +
echo "\n"               выводит просто н а должен \n +
echo 1 2 3 |            +                              ; долже нсинаткс еррор -
ls |       +                                                                 -
  >> не должно быть сеги +
echo < > 123      + син кррор надо
echo aaa < > 123   +    син еррор
| | подряд   +
\   син еррор +


[is_valid_line]
echo 1 2 3 ;                                |           ;лоыит сегу
 echo '$PWD"                                    валидация строки нужна синт еррор
 echo '' сега

[lexer_result]
echo 1 2 3 ;; син еррор
    >                   -

 > her          должкен создать пустой файл
echo 1 2 3 | < her
echo $                                          вывод доллара долже н а у нас сега
e''cho       -
 echo                                           сега , а должная быть пустая строка
    на всех редиректах
 \\\\>\\\>\\ - дичь какая
echo aaa<>123      создает файл 123 и выводит

-no subject
echo ~                  должная обрабатываться
